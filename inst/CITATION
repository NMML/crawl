citHeader("To cite crawl in publications use:")

citEntry(entry = "Misc",
  title        = "{crawl}: Fit Continuous-Time Correlated Random Walk Models to Animal Movement Data",
  author       = personList(as.person("Devin S. Johnson"),
                   as.person("Josh M. London"), 
                   as.person("Kenady Wilson")),
  year         = "2016",
  url          = "https://cran.r-project.org/package=crawl",

  textVersion  =
  paste("Johnson, Devin S., J.M. London, K. Wilson (2016).",
        "crawl: Fit Continuous-Time Correlated Random Walk Models to Animal Movement Data",
        "version 2.0.1",
        "URL https://cran.r-project.org/package=crawl"
        )
