Package: crawl
Type: Package
Title: Fit Continuous-Time Correlated Random Walk Models to Animal Movement Data
Version: 2.3.0
Date: 2022-10-06
Authors@R: c(person("Devin S.", "Johnson", email = "devin.johnson@noaa.gov",
                  role = c("aut", "cre")),
                  person("Josh", "London", email = "josh.london@noaa.gov",
